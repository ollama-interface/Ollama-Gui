{
	"name": "ollama-interface",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"clean-target": "rm -rf ./src-tauri/target",
		"build": "tsc && vite build",
		"build:app:win64": "tauri build --target x86_64-pc-windows-msvc",
		"build:app:silicon": "pnpm clean-target && tauri build --target aarch64-apple-darwin",
		"build:app:intell": "pnpm clean-target && tauri build --target x86_64-apple-darwin",
		"build:app:universal": "pnpm clean-target && tauri build --target universal-apple-darwin",
		"preview": "vite preview",
		"format": "prettier --write \"src/**/*.{ts,tsx,md,css,scss}\"",
		"tauri": "tauri"
	},
	"dependencies": {
		"@radix-ui/react-alert-dialog": "^1.0.4",
		"@radix-ui/react-checkbox": "^1.0.4",
		"@radix-ui/react-dialog": "^1.0.4",
		"@radix-ui/react-dropdown-menu": "^2.0.5",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-select": "^1.2.2",
		"@radix-ui/react-slot": "^1.0.2",
		"@radix-ui/react-toast": "^1.1.4",
		"@radix-ui/react-tooltip": "^1.0.6",
		"@tauri-apps/api": "^1.4.0",
		"@uiw/react-textarea-code-editor": "^2.1.7",
		"@vitejs/plugin-react": "^4.0.4",
		"autoprefixer": "^10.4.16",
		"axios": "^1.5.0",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"dayjs": "^1.11.9",
		"idb": "^8.0.0",
		"immutable": "^4.3.6",
		"jotai": "^2.8.0",
		"lucide-react": "^0.279.0",
		"neverthrow": "^6.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet": "^6.1.0",
		"react-markdown": "^9.0.1",
		"react-syntax-highlighter": "^15.5.0",
		"simple-core-state": "^0.0.16",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.7",
		"ts-pattern": "^5.1.1"
	},
	"devDependencies": {
		"@tauri-apps/cli": "^1.4.0",
		"@types/ndjson": "^2.0.1",
		"@types/node": "^20.6.2",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@types/react-helmet": "^6.1.6",
		"@vitejs/plugin-react": "^4.0.3",
		"ndjson": "^2.0.0",
		"postcss": "^8.4.29",
		"prettier": "3.2.5",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.0.2",
		"vite": "^4.4.4"
	}
}
